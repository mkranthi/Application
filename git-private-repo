pipeline {
    agent any
    parameters {
    gitParameter branchFilter: 'origin/(.*)',defaultValue: 'main', name: 'BRANCH', type: 'PT_BRANCH'
    }
    stages {
        stage("cred"){
            steps{
                withCredentials([gitUsernamePassword(credentialsId: 'githubcred', gitToolName: 'Default')])
            }
        }
    stage('select branch from parameter') {
            steps {
        git branch: "${params.BRANCH}", url: 'https://github.com/kranti-org/itsol-noc-docker.git'
        }
            }     
   }
   }
